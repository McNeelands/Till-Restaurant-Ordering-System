<!-- Modal LOGIN -->
<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <br>
        <h2 class="text-center"><img src="/assets/work/logo_dark.png" class="modal_logo"><br></h2>
      </div>
      <div class="modal-body row text-center">
        <h6 class="text-center">Sign In</h6>
        <%= form_for(:session, url: login_path, :html => { :id => "log_in_form" }) do |f| %>
        <%= f.email_field :email, class: "log_in_form_fields", :placeholder => "Email" %>
        <br></br>
        <%= f.password_field :password, class: "log_in_form_fields", :placeholder => "Password" %>
        <br></br>
        <%= f.submit "Login", class: "log_in_btn_submit" %>
        <% end %>
      </div>
      <a data-dismiss="modal" data-toggle="modal" data-target="#signupModal" id="new_usr_link"><p class="text-center">New? Sign up.</p></a>
      <div class="modal-footer">
        <h6 class="text-center">Thanks for checking out TILL.</h6>
      </div>
    </div>
  </div>
</div>


<!-- Modal SIGNUP -->
<div class="modal fade" id="signupModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h2 class="text-center"><img src="/assets/work/logo.png" class="modal_logo"></h2>
      </div>
      <div class="modal-body row text-center">
        <h6 class="text-center">Sign up for TILL</h6>
        <%= form_for(@user) do |f| %>
        <%= f.text_field :first_name, class: "log_in_form_fields", :placeholder => "First name" %>
        <br></br>
        <%= f.text_field :last_name, class: "log_in_form_fields", :placeholder => "Last name" %>
        <br></br>
        <%= f.email_field :email, class: "log_in_form_fields", :placeholder => "Email" %>
        <br></br>
        <%= f.password_field :password, class: "log_in_form_fields", :placeholder => "Password" %>
        <br></br>
        <%= f.submit "Create an account", class: "log_in_btn_submit" %>
        <% end %>
      </div>
      <div class="modal-footer">
        <h6 class="text-center">No Spam. We Promise!</h6>
      </div>
    </div>
  </div>
</div>

